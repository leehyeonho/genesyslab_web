<html>
<head>
<title></title>
</head>
<body>
  <div id="boardContent">
  <table width="100%" cellpadding="0" cellspacing="0" border="0" class="pub_table mt40">
    <colgroup>
      <col width="6%">
			<col width="*">
    </colgroup>
    <tr>
      <td height="1" bgcolor="#0a51ba" colspan="2"></td>
    </tr>
    <% for (var i = 0; i <= data.length; i++) { %>

      <tr height="20" align="center" bgcolor="#ffffff"></tr>
    <tr>
			<th style="text-decoration: underline;"><%= i+1 %></th>
      <td>
        <p class="pint01" style="font-family: tahoma; font-size: 10pt; font-weight: bold;">
          <%= data[i].content %>
        </p>
				<div id="DivContents" style="width:100%;word-break:break-all;" class="mt5">
					<p>
            <span style="font-family: tahoma; font-size: 10pt;"><%= data[i].author %></span>
          </p>
          <p>
            <span style="font-family: tahoma; font-size: 10pt;"><%= data[i].reg %></span>
          </p>
          <br>
        </div>
      </td>
    </tr>
    <tr>
      <td height="1" bgcolor="#aaaaaa" colspan="2"></td>
    </tr>
    <% } %>
</table>
</div>
</body>
</html>
