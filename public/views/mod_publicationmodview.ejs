<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
</head>
<script type="text/javascript">
function frm_check() {
  if(frm_pubadd.content.value == "") {
    alert("TITLE을 입력해주세요.");
    frm_pubdel.content.focus();
    return false;
  } else if(frm_pubdel.year.value == "") {
    alert("YEAR를 입력해주세요.");
    frm_pubdel.year.focus();
    return false;
  } else if(frm_pubdel.reg.value == "") {
    frm_pubdel.journal.focus();
    alert("JOURNAL을 입력해주세요.");
    return false;
  } else if(frm_pubdel.author.value == "") {
    frm_pubdel.author.focus();
    alert("AUTHOR을 입력해주세요.");
    return false;
  }
  else return true;
}
</script>
<script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
<body>
  <form action="/mod_pubdel" method="post" name="frm_pubdel" id="frm_pubdel">
    <input type="hidden" name="id" id="id" value="<%= data[0].id %>">
    <label><h4>YEAR</h4>
    <input type="text" name="year" id="year" value="<%= data[0].year %>">
    </label>
    <label><h4>TITLE</h4>
    <input type="text" name="content" id="content" value="<%= data[0].content %>">
    </label>
    <label><h4>AUTHOR</h4>
    <input type="text" name="author" id="author" value="<%= data[0].author %>">
    </label>
      <label><h4>JOURNAL</h4>
        <input type="text" name="reg" id="reg" value="<%= data[0].reg %>">
      </label>
<input type="submit" name="submitForm" value="제출">
</form>
</body>
</html>
